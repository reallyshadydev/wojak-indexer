# Database Configuration
DB_USER=postgres
DB_PASSWORD=your_db_password
DB_HOST=localhost
DB_PORT=5432
DB_NAME=wojak_indexer

# WojakCoin RPC Configuration
RPC_BASE_URL=http://127.0.0.1:20760/
RPC_USER=wojakcoinrpc
RPC_PASSWORD=your_rpc_password
RPC_COOKIE_PATH=/root/.wojakcoin/.cookie

# Node RPC URL (for direct RPC calls)
NODE_RPC_URL=http://127.0.0.1:20760

# Electrs API URL (for balance validation)
ELECTRS_API_URL=https://api.wojakcoin.cash

# API Server Configuration
API_PORT=3070

# Rate Limiting
USE_RATE_LIMIT=false
